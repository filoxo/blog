  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop -o",
    "test": "echo \"Error: no test specified\" && exit 1",
    "predeploy": "gatsby build --prefix-paths",
    "deploy:local": "yarn predeploy && gh-pages -d public -b master -r git@github.com:filoxo/filoxo.github.io.git --git git",
    "deploy": "gh-pages -d public -b master -r https://$GITHUB_TOKEN@github.com/filoxo/filoxo.github.io.git --git git",
    "clean": "gatsby clean",
    "post": "run(){ mkdir -p ./src/posts/$1 && touch ./src/posts/$1/index.mdx && echo \"---\ntitle: $1\ndate: 2020-MM-DD\ntags: []\n---\n\" >> ./src/posts/$1/index.mdx; }; run"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.7",
    "autoprefixer": "^10.4.0",
    "gatsby-plugin-postcss": "^5.2.0",
    "postcss": "^8.4.1",
    "prettier": "^2.5.0",
    "tailwindcss": "^2.2.19"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "arrowParens": "always"
  }
