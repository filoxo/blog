{
  "name": "filoblog",
  "description": "My personal blog",
  "version": "2.0.0",
  "author": "Carlos Filoteo",
  "private": true,
  "dependencies": {
    "@mdx-js/mdx": "^0.16.8",
    "@mdx-js/tag": "^0.16.8",
    "gatsby": "^2.0.53",
    "gatsby-image": "^2.0.20",
    "gatsby-mdx": "^0.3.5",
    "gatsby-plugin-catch-links": "^2.0.9",
    "gatsby-plugin-google-analytics": "^2.0.17",
    "gatsby-plugin-manifest": "^2.0.9",
    "gatsby-plugin-offline": "^2.0.16",
    "gatsby-plugin-react-helmet": "^3.0.4",
    "gatsby-plugin-sharp": "^2.0.12",
    "gatsby-remark-images": "^3.0.1",
    "gatsby-source-filesystem": "^2.0.8",
    "gatsby-transformer-sharp": "^2.1.8",
    "mdx-utils": "^0.0.2",
    "prism-react-renderer": "^0.1.5",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-icons": "^3.3.0"
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop -o",
    "format": "prettier --write",
    "test": "echo \"Error: no test specified\" && exit 1",
    "predeploy": "gatsby build --prefix-paths",
    "deploy": "gh-pages -d public -b master -r git@github.com:filoxo/filoxo.github.io.git"
  },
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.1",
    "prettier": "^1.15.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "yarn run format",
      "git add"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "arrowParens": "always"
  }
}
